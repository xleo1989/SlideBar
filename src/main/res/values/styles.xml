<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="style_seekBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/seekbar_layerlist</item>
        <item name="android:indeterminateDrawable">@drawable/seekbar_layerlist</item>
        <item name="android:thumb">@drawable/seekbar_thumb</item>
        <item name="android:thumbnail">@drawable/seekbar_thumb</item>
        <item name="android:focusable">true</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>
</resources>